var literalesAvisoCookies=new Array();function getCookieAvisoLegalCookies(c_name){var c_value=document.cookie;var c_start=c_value.indexOf(" "+c_name+"=");if(c_start==-1){c_start=c_value.indexOf(c_name+"=");}
if(c_start==-1){c_value=null;}else{c_start=c_value.indexOf("=",c_start)+1;var c_end=c_value.indexOf(";",c_start);if(c_end==-1){c_end=c_value.length;}
c_value=unescape(c_value.substring(c_start,c_end));}
return c_value;}
function getMeta(metaName){const metas=document.getElementsByTagName('meta');for(i=0;i<metas.length;i++){if(metas[i].getAttribute('name')===metaName){return metas[i].getAttribute('content');}}
return'';}
function checkCookieAvisoLegalCookies(){var cookieVal=getCookieAvisoLegalCookies('acceptCookies');return(cookieVal!=null&&cookieVal!="");}
function checkCookieEntradaExterna(){var cookieVal=getCookieAvisoLegalCookies('entradaExterna');return(cookieVal!=null&&cookieVal!="");}
function checkAvisoLegalAutoRedirect(){return(document.getElementById('autoRedirectAvisoLegalCookies')!=null);}
function compruebaIdioma(){idiomaCookies=getMeta("avisocookies");if(idiomaCookies.indexOf("EN")!=-1){literalesAvisoCookies[0]='We use our own and third party cookies for statistical studies and personalised advertising through analysis of your browsing. If you continue browsing, you accept the use of cookies. You can change the settings or obtain more information ';literalesAvisoCookies[1]='We use our own and third party cookies for statistical studies and personalised advertising through analysis of your browsing. If you continue browsing, you accept the use of cookies. You can change the settings or obtain more information ';literalesAvisoCookies[2]=' here';literalesAvisoCookies[3]='/english-expats/cookie-policy.html';literalesAvisoCookies[4]='Continue browsing';}else if(idiomaCookies.indexOf("DE")!=-1){literalesAvisoCookies[0]='Wir verwenden eigene und fremde Cookies f\u00fcr statistische Zwecke und personalisierte Werbung anhand der Analyse Ihres Surfverhaltens. Wenn Sie mit Ihrem Besuch der Website fortfahren, sind Sie mit ihrer Nutzung einverstanden.';literalesAvisoCookies[1]='Wir verwenden eigene und fremde Cookies f\u00fcr statistische Zwecke und personalisierte Werbung anhand der Analyse Ihres Surfverhaltens. Wenn Sie mit Ihrem Besuch der Website fortfahren, sind Sie mit ihrer Nutzung einverstanden.';literalesAvisoCookies[2]=' Hier ist es m\u00f6glich, die Einstellungen zu \u00e4ndern oder weitere Informationen zu erhalten';literalesAvisoCookies[3]='/english-expats/cookie-policy.html';literalesAvisoCookies[4]='Weitersurfen';}else if(idiomaCookies.indexOf("CA")!=-1){literalesAvisoCookies[0]='Fem servir cookies pr\u00f2pies i de tercers per a estudis estad\u00edstics i publicitat personalitzada mitjan\u00e7ant l\u00b4an\u00e0lisi de la vostra navegaci\u00f3. Si continueu navegant, n\u00b4accepteu l\u00b4\u00fas. Podeu canviar la configuraci\u00f3 o obtenir m\u00e9s informaci\u00f3 ';literalesAvisoCookies[1]='Fem servir cookies pr\u00f2pies i de tercers per a estudis estad\u00edstics i publicitat personalitzada mitjan\u00e7ant l\u00b4an\u00e0lisi de la vostra navegaci\u00f3. Si continueu navegant, n\u00b4accepteu l\u00b4\u00fas. Podeu canviar la configuraci\u00f3 o obtenir m\u00e9s informaci\u00f3 ';literalesAvisoCookies[2]=' aqu\u00ed';literalesAvisoCookies[3]='/cat/politica-cookies.html';literalesAvisoCookies[4]='Seguir navegant';}else{literalesAvisoCookies[0]='Utilizamos cookies propias y de terceros para estudios estad\u00edsticos y publicidad personalizada a trav\u00e9s del an\u00e1lisis de tu navegaci\u00f3n. Si contin\u00FAas navegando, aceptas su uso. Es posible cambiar la configuraci\u00f3n u obtener m\u00e1s informaci\u00f3n ';literalesAvisoCookies[1]='Utilizamos cookies propias y de terceros para estudios estad\u00edsticos y publicidad personalizada a trav\u00e9s del an\u00e1lisis de tu navegaci\u00f3n. Si contin\u00FAas navegando, aceptas su uso. Es posible cambiar la configuraci\u00f3n u obtener m\u00e1s informaci\u00f3n ';literalesAvisoCookies[2]=' aqu\u00ed';literalesAvisoCookies[3]='/politica-cookies.html';literalesAvisoCookies[4]='Seguir navegando';}}
function showCookiesMsg(){var host=window.location.host;var domain=host.substring(host.indexOf("."),host.length);if(!checkCookieAvisoLegalCookies()){if(document.referrer.indexOf(host)!=-1&&!checkAvisoLegalAutoRedirect()&&!checkCookieEntradaExterna()){if((!document.URL.endsWith("politica-cookies.html"))&&(!document.URL.endsWith("politica-cookies-personalizar.html"))&&(!document.URL.endsWith("cookie-policy.html"))&&(!document.URL.endsWith("cookie-policy-customising.html")))
{var expires=new Date();expires.setDate(expires.getDate()+180);document.cookie="acceptCookies=yes;expires="+expires.toGMTString()+";domain="+domain+";path=/";}}else{if(checkCookieEntradaExterna()){document.cookie="entradaExterna=yes;expires="+new Date(1970).toUTCString()+";domain="+domain+";path=/";}
if(document.getElementById("accept_cookies")==null){compruebaIdioma();var cookiesMsgDiv=document.createElement("div");cookiesMsgDiv.style.cssText="padding-left: 0 !important;padding-right: 0 !important; font-size: 12px; max-width: 1200px;position:relative;background-color:#333f48; color:#ffffff; font-family:Verdana,Helvetica,Tahoma,Arial,sans-serif; font-size:15px; padding-top:1%;padding-bottom:1%; display:block; z-index:998;";cookiesMsgDiv.id="accept_cookies";var mainSpan=document.createElement("span");var msg=literalesAvisoCookies[0];if(window.innerWidth<321){msg=literalesAvisoCookies[1];}
var cookiesMsg=document.createTextNode(msg);a=document.createElement('a');a.setAttribute('href',literalesAvisoCookies[3]);a.setAttribute('onclick',"try{sendEvent('RGPD','PrimeraCapa','PoliticaDeCookies');}catch(error) {}acceptCookie();");a.style.cssText="color:#f6be00; text-decoration:underline;";a.innerHTML=literalesAvisoCookies[2];mainSpan.style.cssText="display:block; margin:0 auto; text-align:justify; width:85%;";var spanExclamation=document.createElement("span");spanExclamation.style.cssText="float:left; font-family:verdana; font-size:3em;margin-right:10px; color:#0000aa;position: relative; top: -6px;";var spanExclamationContent=document.createTextNode("");spanExclamation.appendChild(spanExclamationContent);var aAspa=document.createElement('a');aAspa.style.cssText="color: rgb(246, 190, 0); text-decoration: underline;";aAspa.setAttribute('href','javascript:void(0);');aAspa.setAttribute('onclick',"try{sendEvent('RGPD','PrimeraCapa','SeguirNavegando');}catch(error) {}acceptCookie();");var aSpanAspa=document.createElement('span');aSpanAspa.title="Seguir navegando";if(detectmob()){aSpanAspa.style.cssText="float: right; font-family: verdana; margin-left: 10px; position: relative; line-height: 1.1em; border: 1px solid;padding: 2px;color: #ffffff; background-color:#F6BE00;top: 0px;width:90px;text-align:center";}else{aSpanAspa.style.cssText="float: right; font-family: verdana; margin-left: 10px; position: relative; line-height: 1.1em; border: 1px solid;padding: 2px;color: #ffffff; background-color:#F6BE00;top: 0px;width:90px;text-align:center";}
var spanAspa=document.createTextNode(literalesAvisoCookies[4]);aSpanAspa.appendChild(spanAspa);aAspa.appendChild(aSpanAspa);mainSpan.appendChild(spanExclamation);mainSpan.appendChild(aAspa);mainSpan.appendChild(cookiesMsg);mainSpan.appendChild(a);cookiesMsg=document.createTextNode(".");mainSpan.appendChild(cookiesMsg);cookiesMsgDiv.appendChild(mainSpan);document.body.insertBefore(cookiesMsgDiv,document.body.firstChild);}}}}
function acceptCookie()
{if((!document.URL.endsWith("politica-cookies.html"))&&(!document.URL.endsWith("politica-cookies-personalizar.html"))&&(!document.URL.endsWith("cookie-policy.html"))&&(!document.URL.endsWith("cookie-policy-customising.html")))
{var host=window.location.host;var domain=host.substring(host.indexOf("."),host.length);document.getElementById('accept_cookies').style.display="none";var expires=new Date();expires.setDate(expires.getDate()+180);document.cookie="acceptCookies=yes;expires="+expires.toGMTString()+";domain="+domain+";path=/";}}
showCookiesMsg();function detectmob(){if(navigator.userAgent.match(/Android/i)||navigator.userAgent.match(/webOS/i)||navigator.userAgent.match(/iPhone/i)||navigator.userAgent.match(/iPad/i)||navigator.userAgent.match(/iPod/i)||navigator.userAgent.match(/BlackBerry/i)||navigator.userAgent.match(/Windows Phone/i)){return true;}
else{return false;}}